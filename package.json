{
  "name": "node-pty",
  "description": "Fork pseudoterminals in Node.JS",
  "author": {
    "name": "Daniel Imms"
  },
  "version": "0.6.11",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/edjubuh/node-pty.git"
  },
  "files": [
    "lib/"
  ],
  "homepage": "https://github.com/edjubuh/node-pty",
  "bugs": {
    "url": "https://github.com/edjubuh/node-pty/issues"
  },
  "keywords": [
    "pty",
    "tty",
    "terminal",
    "pseudoterminal",
    "forkpty",
    "openpty"
  ],
  "scripts": {
    "tsc": "tsc",
    "tslint": "tslint src/**/*.ts",
    "install": "node-pre-gyp install --falback-to-build",
    "test": "cross-env NODE_ENV=test mocha -R spec test",
    "prepublish": [
      "npm run tsc",
      "npm ls"
    ],
    "postpublish": "node-pre-gyp-github publish --release"
  },
  "dependencies": {
    "nan": "2.5.0"
  },
  "devDependencies": {
    "@types/node": "^6.0.58",
    "cross-env": "^3.1.3",
    "mocha": "^3.1.2",
    "tslint": "^4.3.1",
    "typescript": "^2.1.4",
    "node-pre-gyp": "0.6.x",
    "node-pre-gyp-github": "1.3.x"
  },
  "bundledDependencies": ["node-pre-gyp"],
  "binary": {
    "module_name": "pty",
    "module_path": "./publish",
    "host": "https://github.com/edjubuh/node-pty/releases/download/",
    "remote_path": "{version}"
  }
}
